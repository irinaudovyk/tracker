{
  "name": "Tracker",
  "version": "0.0.1",
  "description": "Tracker",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && bower install --config.interactive=false --allow-root && gulp",
    "setup.dev": "npm install && bower install --config.interactive=false --allow-root && gulp dev",
    "start.prod": "NODE_ENV=prod pm2 restart app.js",
    "start.dev": "nodemon -e js -e json --delay 100ms -i node_modules/ -i public/ -i features/ -w app.js -w app app.js",
    "start.dev.debug": "nodemon -e js -e json --delay 200ms -i node_modules/ -i public/ -i features/ -w app.js -w app app.js --debug=5858",
    "test": "NODE_ENV=development node_modules/.bin/cucumber.js --format pretty",
    "test.wip": "NODE_ENV=development node_modules/.bin/cucumber.js --format pretty -t @wip",
    "docker.dev": "nodemon -i public/ -i features/ -x ./docker.dev.sh",
    "docker.console": "docker-compose run --rm dev bash",
    "docker.test": "docker-compose run --rm dev npm test",
    "docker.test.wip": "docker-compose run --rm dev npm run test.wip",
    "docker.stop": "docker-compose stop"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^0.9.2",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^0.7.0",
    "cookie-parser": "^1.4.3",
    "cron": "^1.0.9",
    "express": "^4.14.0",
    "express-form": "^0.12.5",
    "express-session": "^1.13.0",
    "fs-extra": "^0.18.4",
    "gridfs-stream": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-if": "^1.2.5",
    "gulp-jscs": "^2.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-rev-append": "^0.1.6",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.0",
    "gulp-useref": "^1.3.0",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "moment": "^2.13.0",
    "mongoose": "4.5.9",
    "multer": "^0.1.8",
    "nodemailer": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google": "^0.3.0",
    "passport-google-oauth": "^0.2.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "passport.socketio": "git+https://github.com/metacommunications/passport.socketio",
    "plus.application": "^1.1.8",
    "pow-mongodb-fixtures": "0.13.0",
    "socket.io": "1.4.8"
  },
  "devDependencies": {
    "async": "^0.9.2",
    "chai": "^3.0.0",
    "chainit3": "^3.1.2",
    "cucumber": "^0.5.0",
    "cucumber.usesteps": "0.0.9",
    "lodash": "^3.9.3",
    "plus.application": "^1.1.0",
    "pow-mongodb-fixtures": "^0.10.0",
    "selenium-webdriver": "2.46.0",
    "supertest": "^1.0.1",
    "webdriver-sizzle": "^0.2.2"
  }
}
